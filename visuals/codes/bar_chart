import pandas as pd
import matplotlib.pyplot as plt

df = pd.read_csv("../../data/sudoku_performance.csv")
plt.figure(figsize=(10, 6))

plt.bar(df["Tarih"], df["Kolay Kazanma Oranı (%)"], label="Easy Win Rate",alpha=0.7)
plt.bar(df["Tarih"], df["Zor Kazanma Oranı (%)"], label="Hard Win Rate", alpha=0.7)

plt.title("Easy vs Hard Win Rates")
plt.xlabel("Date")
plt.ylabel("Win Rate (%)")
plt.xticks(rotation=45)
plt.legend()
plt.tight_layout()


